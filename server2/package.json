{
  "name": "server2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:dev:api-gateway": "cross-env API_GATEWAY_PORT=3000 RABBITMQ_URL=amqp://localhost:5672 nest start --watch api-gateway",
    "start:dev:chat": "cross-env CHAT_API_PORT=3001 RABBITMQ_URL=amqp://localhost:5672 nest start --watch chat",
    "start:dev:enrollment": "cross-env ENROLLMENT_API_PORT=3002 RABBITMQ_URL=amqp://localhost:5672 nest start --watch enrollment",
    "start:dev:system-management": "cross-env SYSTEM_MANAGEMENT_API_PORT=3003 RABBITMQ_URL=amqp://localhost:5672 nest start --watch system-management",
    "start:dev:metrics": "cross-env METRICS_API_PORT=3004 RABBITMQ_URL=amqp://localhost:5672 nest start --watch metrics",
    "start:dev:all": "concurrently \"npm:start:dev:api-gateway\" \"npm:start:dev:chat\" \"npm:start:dev:enrollment\" \"npm:start:dev:system-management\" \"npm:start:dev:metrics\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/server2/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server2/test/jest-e2e.json",
    "prisma:schema": "npm run prisma:pull && npm run prisma:generate",
    "prisma:generate": "npx prisma generate --schema=libs/prisma/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=libs/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/terminus": "^11.0.0",
    "@opentelemetry/api": "^1.9.0",
    "@opentelemetry/auto-instrumentations-node": "^0.57.0",
    "@opentelemetry/exporter-trace-otlp-http": "^0.200.0",
    "@opentelemetry/instrumentation-express": "^0.48.0",
    "@opentelemetry/instrumentation-http": "^0.200.0",
    "@opentelemetry/instrumentation-pg": "^0.52.0",
    "@opentelemetry/sdk-node": "^0.200.0",
    "@prisma/client": "^6.5.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.5",
    "dotenv": "^16.4.7",
    "opossum": "^5.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/amqplib": "^0.10.7",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/"
    ]
  }
}
