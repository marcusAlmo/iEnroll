{
  "name": "server2",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "init:rabbitmq": "cross-env RABBITMQ_URL=amqp://localhost:5672",
    "start:dev:api-gateway": "nest start --watch api-gateway",
    "start:dev:service": "npm run init:rabbitmq && nest start --watch",
    "start:dev:chat": "npm run start:dev:service chat",
    "start:dev:enrollment": "npm run start:dev:service enrollment",
    "start:dev:system-management": "npm run start:dev:service system-management",
    "start:dev:metrics": "npm run start:dev:service metrics",
    "start:dev:auth": "npm run start:dev:service auth",
    "start:dev:file": "npm run start:dev:service file",
    "start:dev:image": "npm run start:dev:service image",
    "start:dev:all": "concurrently --prefix name --names \"api-gateway,chat,enrollment,system-management,metrics,auth,file,image\" --prefix-colors \"cyan,magenta,blue,green,gray,yellow,red,white\" \"npm run start:dev:api-gateway\" \"npm run start:dev:chat\" \"npm run start:dev:enrollment\" \"npm run start:dev:system-management\" \"npm run start:dev:metrics\" \"npm run start:dev:auth\" \"npm run start:dev:file\" \"npm run start:dev:image\"",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/server2/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/server2/test/jest-e2e.json",
    "prisma:schema": "npm run prisma:pull && npm run prisma:generate",
    "prisma:generate": "npx prisma generate --schema=libs/prisma/prisma/schema.prisma",
    "prisma:pull": "npx prisma db pull --schema=libs/prisma/prisma/schema.prisma"
  },
  "dependencies": {
    "@godaddy/terminus": "^4.12.1",
    "@nestjs/common": "^11.0.1",
    "@nestjs/config": "^4.0.1",
    "@nestjs/core": "^11.0.1",
    "@nestjs/jwt": "^11.0.0",
    "@nestjs/mapped-types": "*",
    "@nestjs/microservices": "^11.0.12",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^11.0.1",
    "@nestjs/serve-static": "^5.0.3",
    "@nestjs/swagger": "^11.1.4",
    "@nestjs/terminus": "^11.0.0",
    "@prisma/client": "^6.5.0",
    "amqp-connection-manager": "^4.1.14",
    "amqplib": "^0.10.7",
    "bcrypt": "^5.1.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dotenv": "^16.4.7",
    "opossum": "^5.0.1",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sanitize-filename": "^1.6.3",
    "sharp": "^0.34.1",
    "swagger-ui-express": "^5.0.1",
    "tesseract.js": "^6.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.18.0",
    "@nestjs/cli": "^11.0.0",
    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.1",
    "@swc/cli": "^0.6.0",
    "@swc/core": "^1.10.7",
    "@types/amqplib": "^0.10.7",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^5.0.1",
    "@types/jest": "^29.5.14",
    "@types/multer": "^1.4.12",
    "@types/node": "^22.10.7",
    "@types/passport-jwt": "^4.0.1",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^10.0.0",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "eslint": "^9.18.0",
    "eslint-config-prettier": "^10.0.1",
    "eslint-plugin-prettier": "^5.2.2",
    "globals": "^16.0.0",
    "jest": "^29.7.0",
    "prettier": "^3.4.2",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-loader": "^9.5.2",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.20.0"
  },
  "jest": {
    "silent": false,
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "./coverage",
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@lib/(.*)$": "<rootDir>/libs/$1"
    }
  }
}
