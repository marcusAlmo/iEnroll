{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/api-gateway/src",
  "compilerOptions": {
    "deleteOutDir": true,
    "webpack": true,
    "webpackConfigPath": "webpack.config.js",
    "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
  },
  "monorepo": true,
  "root": ".",
  "projects": {
    "address": {
      "type": "library",
      "root": "libs/address",
      "entryFile": "index",
      "sourceRoot": "libs/address/src",
      "compilerOptions": {
        "tsConfigPath": "libs/address/tsconfig.lib.json"
      }
    },
    "api-gateway": {
      "type": "application",
      "root": "apps/api-gateway",
      "entryFile": "main",
      "sourceRoot": "apps/api-gateway/src",
      "compilerOptions": {
        "tsConfigPath": "apps/api-gateway/tsconfig.app.json"
      }
    },
    "auth": {
      "type": "application",
      "root": "apps/auth",
      "entryFile": "main",
      "sourceRoot": "apps/auth/src",
      "compilerOptions": {
        "tsConfigPath": "apps/auth/tsconfig.app.json"
      }
    },
    "chat": {
      "type": "application",
      "root": "apps/chat",
      "entryFile": "main",
      "sourceRoot": "apps/chat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/chat/tsconfig.app.json"
      }
    },
    "dtos": {
      "type": "library",
      "root": "libs/dtos",
      "entryFile": "index",
      "sourceRoot": "libs/dtos/src",
      "compilerOptions": {
        "tsConfigPath": "libs/dtos/tsconfig.lib.json"
      }
    },
    "enrollment": {
      "type": "application",
      "root": "apps/enrollment",
      "entryFile": "main",
      "sourceRoot": "apps/enrollment/src",
      "compilerOptions": {
        "tsConfigPath": "apps/enrollment/tsconfig.app.json"
      }
    },
    "file": {
      "type": "application",
      "root": "apps/file",
      "entryFile": "main",
      "sourceRoot": "apps/file/src",
      "compilerOptions": {
        "tsConfigPath": "apps/file/tsconfig.app.json"
      }
    },
    "image": {
      "type": "application",
      "root": "apps/image",
      "entryFile": "main",
      "sourceRoot": "apps/image/src",
      "compilerOptions": {
        "tsConfigPath": "apps/image/tsconfig.app.json"
      }
    },
    "metrics": {
      "type": "application",
      "root": "apps/metrics",
      "entryFile": "main",
      "sourceRoot": "apps/metrics/src",
      "compilerOptions": {
        "tsConfigPath": "apps/metrics/tsconfig.app.json"
      }
    },
    "system-management": {
      "type": "application",
      "root": "apps/system-management",
      "entryFile": "main",
      "sourceRoot": "apps/system-management/src",
      "compilerOptions": {
        "tsConfigPath": "apps/system-management/tsconfig.app.json"
      }
    }
  }
}